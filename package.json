{
  "name": "healthymama-monorepo",
  "private": true,
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@healthymama/web",
    "dev:mobile": "turbo run dev --filter=@healthymama/mobile",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@healthymama/web",
    "lint": "turbo run lint",
    "check": "turbo run check",
    "convex:dev": "turbo run convex:dev --filter=@healthymama/convex",
    "convex:deploy": "turbo run convex:deploy --filter=@healthymama/convex"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "next": "16.0.10",
    "turbo": "^2.3.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/metro-runtime": "^6.1.2",
    "@react-native/assets-registry": "^0.83.1",
    "@react-native/js-polyfills": "^0.83.1",
    "@react-native/normalize-colors": "^0.83.1",
    "@react-native/virtualized-lists": "^0.83.1",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/core": "^7.13.7",
    "@react-navigation/elements": "^2.9.3",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@react-navigation/routers": "^7.5.3",
    "convex": "^1.31.2",
    "expo-asset": "^12.0.12",
    "expo-constants": "^18.0.12",
    "expo-file-system": "^19.0.21",
    "expo-font": "^14.0.10",
    "expo-modules-core": "^3.0.29",
    "fast-deep-equal": "^3.1.3",
    "filter-obj": "^6.1.0",
    "invariant": "^2.2.4",
    "nanoid": "^5.1.6",
    "nullthrows": "^1.1.1",
    "pretty-format": "^30.2.0",
    "query-string": "^9.3.1",
    "react-is": "^19.2.3",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-is-edge-to-edge": "^1.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-worklets": "^0.7.1",
    "react-refresh": "^0.18.0",
    "scheduler": "^0.27.0",
    "split-on-first": "^4.0.0",
    "use-latest-callback": "^0.3.3",
    "use-sync-external-store": "^1.6.0",
    "whatwg-fetch": "^3.6.20"
  }
}
